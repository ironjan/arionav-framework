<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="de.ironjan.arionav.sample.MapFragment"
        tools:layout="@layout/fragment_map"
        android:label="MapFragment" >

        <action
            android:id="@+id/action_to_mapFragment"
            app:destination="@id/mapFragment" />

        <action android:id="@+id/action_to_nearbyWifiApsFragment"
            app:destination="@+id/nearbyWifiAps" />

        <action android:id="@+id/action_to_nearbyBluetoothTokensFragment"
            app:destination="@+id/nearbyBluetoothTokensFragment" />

        <action android:id="@+id/action_to_poiListFragment"
            app:destination="@+id/poiListFragment" />

        <action android:id="@+id/action_to_providerConfig"
            app:destination="@+id/providerConfig" />


        <action
            android:id="@+id/action_to_arViewFragment"
            app:destination="@id/arViewFragment" />
        <argument
            android:name="selectedPoiCoordinate"
            app:argType="string"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/arViewFragment"
        android:name="de.ironjan.arionav.sample.ArViewFragment"
        tools:layout="@layout/fragment_ar_view"
        android:label="ArViewFragment" >
        <action
            android:id="@+id/action_to_mapFragment"
            app:destination="@id/mapFragment" />

        <action android:id="@+id/action_to_nearbyWifiApsFragment"
            app:destination="@+id/nearbyWifiAps" />

        <action android:id="@+id/action_to_nearbyBluetoothTokensFragment"
            app:destination="@+id/nearbyBluetoothTokensFragment" />

        <action android:id="@+id/action_to_poiListFragment"
            app:destination="@+id/poiListFragment" />

        <action android:id="@+id/action_to_providerConfig"
            app:destination="@+id/providerConfig" />
    </fragment>

    <fragment android:id="@+id/nearbyWifiAps"
        android:name="de.ironjan.arionav.sample.NearbyAccessPointsFragment"
        tools:layout="@layout/fragment_custom_list"
        android:label="NearbyAccessPointsFragment">
        <action
            android:id="@+id/action_to_mapFragment"
            app:destination="@id/mapFragment" />

        <action android:id="@+id/action_to_nearbyWifiApsFragment"
            app:destination="@+id/nearbyWifiAps" />

        <action android:id="@+id/action_to_nearbyBluetoothTokensFragment"
            app:destination="@+id/nearbyBluetoothTokensFragment" />

        <action android:id="@+id/action_to_poiListFragment"
            app:destination="@+id/poiListFragment" />

        <action android:id="@+id/action_to_providerConfig"
            app:destination="@+id/providerConfig" />
    </fragment>
    <fragment
        android:id="@+id/nearbyBluetoothTokensFragment"
        android:name="de.ironjan.arionav.sample.NearbyBluetoothTokensFragment"
        android:label="NearbyBluetoothTokensFragment">

        <action
            android:id="@+id/action_to_mapFragment"
            app:destination="@id/mapFragment" />

        <action android:id="@+id/action_to_nearbyWifiApsFragment"
            app:destination="@+id/nearbyWifiAps" />

        <action android:id="@+id/action_to_nearbyBluetoothTokensFragment"
            app:destination="@+id/nearbyBluetoothTokensFragment" />

        <action android:id="@+id/action_to_poiListFragment"
            app:destination="@+id/poiListFragment" />

        <action android:id="@+id/action_to_providerConfig"
            app:destination="@+id/providerConfig" />
    </fragment>

    <fragment android:id="@+id/poiListFragment"
        android:name="de.ironjan.arionav.sample.PlacesFragment"
        android:label="PoiListFragment" >

        <action
            android:id="@+id/action_to_mapFragment"
            app:destination="@id/mapFragment" />

        <action android:id="@+id/action_to_nearbyWifiApsFragment"
            app:destination="@+id/nearbyWifiAps" />

        <action android:id="@+id/action_to_nearbyBluetoothTokensFragment"
            app:destination="@+id/nearbyBluetoothTokensFragment" />

        <action android:id="@+id/action_to_poiListFragment"
            app:destination="@+id/poiListFragment" />

        <action android:id="@+id/action_to_providerConfig"
            app:destination="@+id/providerConfig" />
    </fragment>

    <fragment android:id="@+id/providerConfig"
        android:name="de.ironjan.arionav.ionav.positioning.config.ProviderConfigFragment"
        android:label="ProviderConfigFragment" >


        <action
            android:id="@+id/action_to_mapFragment"
            app:destination="@id/mapFragment" />

        <action android:id="@+id/action_to_nearbyWifiApsFragment"
            app:destination="@+id/nearbyWifiAps" />

        <action android:id="@+id/action_to_nearbyBluetoothTokensFragment"
            app:destination="@+id/nearbyBluetoothTokensFragment" />

        <action android:id="@+id/action_to_poiListFragment"
            app:destination="@+id/poiListFragment" />

        <action android:id="@+id/action_to_providerConfig"
            app:destination="@+id/providerConfig" />
    </fragment>
</navigation>